  "paths": {
    "/v3/merchants/{mId}/cash_events": {
      "get": {
        "description": "Get all cash events",
        "summary": "GetAllCashEvents",
        "operationId": "V3MerchantsCashEventsByMIdGet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "mId",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Merchant Id"
          },
          {
            "name": "filter",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": ["employee.id", "note", "amountChange", "employee.name", "type", "device.id", "timestamp"]
          },
          {
            "name": "expand",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": ["employee", "device"]
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/CashEvent"
            }
          }
        },
        "security": []
      }
    },
    "/v3/merchants/{mId}/employees/{empId}/cash_events": {
      "get": {
