{
    "openapi": "3.0.0",
    "info": {
        "version": "3",
        "title": "Apps"
    },
    "paths": {
        "/v3/apps/{aId}/merchants/{mId}/billing_info": {
            "get": {
                "description": "Get merchant's billing information for an app",
                "summary": "GetMerchantBillingInfo",
                "operationId": "V3AppsMerchantsBillingInfoByAIdAndMIdGet",
                "parameters": [
                    {
                        "name": "aId",
                        "in": "path",
                        "required": true,
                        "description": "App Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "mId",
                        "in": "path",
                        "required": true,
                        "description": "Merchant Id",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AppBillingInfo"
                                }
                            }
                        }
                    }
                },
                "security": []
            }
        },
        "/v3/apps/{aId}/merchants/{mId}/metereds/{meteredId}": {
            "get": {
                "description": "Get the app metered billing events for an app metered, e.g. all the billing events for the event 'reservation'",
                "summary": "GetMerchantAppMeteredEvents",
                "operationId": "V3AppsMerchantsMeteredsMeteredIdByAIdAndMIdGet",
                "parameters": [
                    {
                        "name": "aId",
                        "in": "path",
                        "required": true,
                        "description": "App Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "mId",
                        "in": "path",
                        "required": true,
                        "description": "Merchant Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "meteredId",
                        "in": "path",
                        "required": true,
                        "description": "Metered Id",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AppMeteredEvent"
                                }
                            }
                        }
                    }
                },
                "security": []
            },
            "post": {
                "description": "Create an app billing metered event",
                "summary": "CreateMerchantAppMeteredEvent",
                "operationId": "V3AppsMerchantsMeteredsMeteredIdByAIdAndMIdPost",
                "parameters": [
                    {
                        "name": "aId",
                        "in": "path",
                        "required": true,
                        "description": "App Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "mId",
                        "in": "path",
                        "required": true,
                        "description": "Merchant Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "meteredId",
                        "in": "path",
                        "required": true,
                        "description": "Metered Id",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AppMeteredEvent"
                                }
                            }
                        }
                    }
                },
                "security": []
            }
        },
        "/v3/apps/{aId}/merchants/{mId}/metereds/{meteredId}/events/{eventId}": {
            "get": {
                "description": "Get an app billing metered event",
                "summary": "GetMerchantAppMeteredEvent",
                "operationId": "V3AppsMerchantsMeteredsMeteredIdEventsEventIdByAIdAndMIdGet",
                "parameters": [
                    {
                        "name": "aId",
                        "in": "path",
                        "required": true,
                        "description": "App Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "mId",
                        "in": "path",
                        "required": true,
                        "description": "Merchant Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "meteredId",
                        "in": "path",
                        "required": true,
                        "description": "Metered Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "eventId",
                        "in": "path",
                        "required": true,
                        "description": "",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AppMeteredEvent"
                                }
                            }
                        }
                    }
                },
                "security": []
            },
            "delete": {
                "description": "Delete an app billing metered event, if not charged.",
                "summary": "DeleteMerchantAppMeteredEvent",
                "operationId": "V3AppsMerchantsMeteredsMeteredIdEventsEventIdByAIdAndMIdDelete",
                "parameters": [
                    {
                        "name": "aId",
                        "in": "path",
                        "required": true,
                        "description": "App Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "mId",
                        "in": "path",
                        "required": true,
                        "description": "Merchant Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "meteredId",
                        "in": "path",
                        "required": true,
                        "description": "Metered Id",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "eventId",
                        "in": "path",
                        "required": true,
